<%= "Kursna lista na dan : #{@data[0]['dateList']} broj #{@data[0]['listNumber']}" %>
<table id="currency" class="table table-striped table-bordered table-condensed ">
      <thead>
        <tr>
            <th class="text-center" >Zemlja</th>
            <th class="text-center">Valuta</th>
            <th class="text-center">Kupovni</th>
            <th class="text-center">Srednji</th>
            <th class="text-center">Prodajni</th>
        </tr>
    </thead>
   <% @data.each do |cur| %>
    <tr>
      <td class="text-center">
        <%= cur["country"] %>
      </td>
      <td class="text-center" >
        <%= cur["currencyId"] %>
      </td>
      <td class="text-right">
        <%= cur["buyCashe"] %>
      </td>
      <td class="text-right">
        <%= cur["stateMiddle"] %>
      </td>
      <td class="text-right">
        <%= cur["sellCashe"] %>
      </td>
    </tr>
  <%end%>
</table>

