<% content_for :main do %>
<h4>Accounts</h4>
  <div class="row">
  <div class="col-xs-12">
    <table class="table table-striped table-bordered">
      <th>Type Desc</th>
      <th>Account</th>
      <th>Balance</th>
      <th>Availble</th>
      <th>Alias</th>
      <th>Type</th>
      <th>Currency</th>
    <% @data.each do |account| %>
    <tr>
      <td><%= account["accountTypeDescription"]  %></td>
      <td><big><%= account["account"]  %></big></td>
      <td class="warning text-right"><%= number_to_currency(account["balance"].to_f,format: "%n") %></td>
      <td class="text-right"><%= number_to_currency( account["availableBalance"].to_f,format: "%n")  %></td>
      <td><%= account["accountAlias"]  %></td>
      <td><%= account["accountType"]  %></td>
      <td><%= account["currencyCode"]  %></td>
      <td><%= button_to 'CashOut',set_accountout_atms_path(acc: account["account"]),method: "post",class:"btn btn-large btn-default"  %></td>
      <td><%= button_to 'CashIn',set_accountin_atms_path(acc: account["account"]),method: "post",class:"btn btn-large btn-default"  %></td>
    </tr>
    <% end %>
  </table>
  </div>
  </div>
<% end %>
