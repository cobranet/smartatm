<% content_for :main do %>
<h4>Reversal</h4>

<%= form_tag("/atms/reversal", method: "post" , class: "form-horizontal") do %>

<div class="form-group">
  <%= label_tag :reservationid, "Id rezervacije" , class: "control-label col-xs-2" %>
  <div class="col-xs-5" %>
    <%= text_field_tag :reservationid,session["lastreserve"],class: "form-control" %>
  </div>
  <div class="col-xs-2" %>
    <%= submit_tag "OK",class:"btn btn-primary btn-large"  %>
  </div>
</div>

<%end%>

<%end%>
