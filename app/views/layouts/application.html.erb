<!DOCTYPE html>
<html>
<head>
  <title>Smartatm</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
   <%= render "navigation" %>
    <div class="container">
      <div class="row">
	<div class="col-md-12">
	  <%= yield :datacard  %>
	</div>
      </div>	
    <div class="row">
      <div class="col-md-12">
	<%= yield :main %>
      </div>
    </div>  
    <div class="err row">
      <div class="col-md-12">
	<%= yield :error %>
      </div>
    </div>  
  </div>

</body>
</html>
